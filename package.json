    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "highcharts": "^12.3.0",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "solid-js": "^1.9.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
